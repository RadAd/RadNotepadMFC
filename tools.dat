<?xml version="1.0" encoding="UTF-8"?>
<tools>
    <tool name="Run">
        <cmd>{file}</cmd>
    </tool>
    <tool name="CMD">
        <cmd>cmd.exe</cmd>
    </tool>
    <tool name="Explorer">
        <cmd>explorer.exe</cmd>
        <param>/select,"{file}"</param>
    </tool>
    <tool name="Google">
        <cmd>https://www.google.com.au/search?q={selected}</cmd>
    </tool>
    <tool name="FindStr" capture="Find">
        <cmd>findstr.exe</cmd>
        <param>/S /N /P /C:"{selected}" *.*</param>
    </tool>
    <tool name="Output" capture="Output">
        <cmd>cmd.exe</cmd>
        <param>/C type {file}</param>
    </tool>
</tools>